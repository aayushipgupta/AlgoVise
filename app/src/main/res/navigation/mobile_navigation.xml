<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:tag="VisualFragment"
        android:name="com.aayushigupta.algovise.ui.home.VisualFragment"
        tools:layout="@layout/fragment_visual" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:tag="DashboardFragment"
        android:name="com.aayushigupta.algovise.ui.dashboard.DashboardFragment"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:tag="NotificationsFragment"
        android:name="com.aayushigupta.algovise.ui.notifications.NotificationsFragment"
        tools:layout="@layout/fragment_notifications" />

</navigation>